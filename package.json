    "vue": "^2.6.0 || ^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@vue/test-utils": "^1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.0",
    "babel-loader": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.0",
    "jest": "^27.5.0",
    "jest-serializer-vue": "^2.0.2",
