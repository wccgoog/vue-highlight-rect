  "scripts": {
    "build": "npm run build:esm && npm run build:common && npm run build:umd",
    "build:esm": "cross-env NODE_ENV=production webpack --config webpack.config.esm.js",
    "build:common": "cross-env NODE_ENV=production webpack --config webpack.config.common.js",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.umd.js",
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.dev.js --watch",
